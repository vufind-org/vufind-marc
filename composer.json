    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.26.1",
        "pear/http_request2": "2.5.1",
        "phpmd/phpmd": "2.13.0",
        "phpstan/phpstan": "1.10.34",
        "phpunit/phpunit": "9.6.12",
        "phing/phing": "2.17.4",
        "squizlabs/php_codesniffer": "3.7.2"
    }
